  "bugs": {
    "url": "https://github.com/Allvaa/nekobot-api/issues"
  },
  "homepage": "https://github.com/Allvaa/nekobot-api#readme",
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "standard-version": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
