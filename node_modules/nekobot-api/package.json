{
  "name": "nekobot-api",
  "version": "2.1.0",
  "description": "NekoBot API wrapper for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ignore-path .gitignore \"**/*.ts\"",
